{
  "files.exclude": {
    "vcpkg/downloads": true,
    "vcpkg/packages": true,
    "vcpkg/ports": true,
    "vcpkg/scripts": true,
    "vcpkg/toolsrc": true,
    "vcpkg/versions": true,
    "vcpkg/triplets": true,
    "build/**": true,
    ".tmp/**": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "cmake.environment": {
    "GEN": "ninja",
    "VCPKG_TOOLCHAIN_PATH": "${workspaceFolder}/vcpkg/scripts/buildsystems/vcpkg.cmake",
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#81cb55",
    "activityBar.background": "#81cb55",
    "activityBar.foreground": "#15202b",
