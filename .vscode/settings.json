    "activityBarBadge.background": "#3c6ec3",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#81cb55",
    "statusBar.background": "#66b538",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#508e2c",
    "statusBarItem.remoteBackground": "#66b538",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#66b538",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#66b53899",
    "titleBar.inactiveForeground": "#15202b99"
